
module Seri.Sudoku.IntegerCell where

import Prelude
import Seri.SMT.SMT

import Seri.Sudoku.Cell

data IntegerCell = IntegerCell Integer
    deriving(Eq)

instance Cell IntegerCell where
    mkCell = IntegerCell

    deCell (IntegerCell x) = x

    freeCell = do
        x <- free
        assert ((x > 0) && (x <= m))
        return (IntegerCell x)

    distinctCell = distinct



