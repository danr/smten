
module Seri.Sudoku.EnumCell where

import Prelude
import Seri.SMT.SMT

import Seri.Sudoku.Cell

data EnumCell = C1 | C2 | C3 | C4
    deriving(Eq, Free)

instance Cell EnumCell where
    mkCell 1 = C1
    mkCell 2 = C2
    mkCell 3 = C3
    mkCell 4 = C4

    deCell C1 = 1
    deCell C2 = 2
    deCell C3 = 3
    deCell C4 = 4

    freeCell = free

    distinctCell = distinct

