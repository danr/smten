
-- Seri declarations for the enoch.hs test case.
module Seri.Enoch.Enoch where

import Prelude
import Seri.SMT.SMT

quadruple :: Integer -> Integer
quadruple a = a + (a + (a + a))

data Foo = Bar Integer
         | Sludge Bool deriving(Free)

defoo :: Foo -> Integer
defoo (Bar x) = x
defoo (Sludge b) = if b then 1 else 0

