
module Seri.SMT.Tests.Sudoku where

import Prelude
import Seri.SMT.SMT
import Seri.Sudoku.Board
import Seri.Sudoku.Boards
import Seri.Sudoku.Solver
import Seri.Sudoku.IntegerCell

qmain :: Query [String]
qmain = do
    board <- (readBoard diabolical :: Query (Board IntegerCell))
    result <- solve board
    case result of
       Satisfiable v -> return (print v)
       _ -> return ["no solution"]

main :: IO ()
main = do
    r <- runYices2 (Just "build/test/Sudoku.yices2.dbg") qmain
    mapM_ putStrLn r

