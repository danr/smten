
module Seri.SMT.Tests.Sudoku5 where

import Prelude
import Seri.SMT.SMT
import Seri.Sudoku.Board
import Seri.Sudoku.Boards
import Seri.Sudoku.Solver
import Seri.Sudoku.EnumCell2

qmain :: Query [[Char]]
qmain = do
    board <- (readBoard diabolical :: Query (Board EnumCell2))
    result <- solve board
    case result of
       Satisfiable v -> return (print v)
       _ -> return ["no solution"]

main :: IO ()
main = do
    r <- runSTP (Just "build/test/Sudoku5.stp.dbg") qmain
    mapM_ putStrLn r

