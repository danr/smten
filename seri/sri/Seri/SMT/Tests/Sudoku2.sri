
module Seri.SMT.Tests.Sudoku2 where

import Prelude
import Seri.SMT.SMT
import Seri.Sudoku.Board
import Seri.Sudoku.Boards
import Seri.Sudoku.Solver
import Seri.Sudoku.EnumCell
import Seri.Sudoku.Cell

qmain :: Query [[Char]]
qmain = do
    board <- (readBoard little :: Query (Board EnumCell))
    result <- solve board
    case result of
       Satisfiable v -> return (print v)
       _ -> return ["no solution"]

main :: IO ()
main = do
    r <- runYices2 (Just "build/test/Sudoku2.yices2.dbg") qmain
    mapM_ putStrLn r

