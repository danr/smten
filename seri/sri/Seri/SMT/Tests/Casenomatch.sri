
module Seri.SMT.Tests.Casenomatch where {

import Prelude;
import Seri.SMT.SMT;

data Foo = Bar
         | Sludge Bool;

defoo :: Foo -> Integer;
defoo f = case f of {
            (Sludge True) -> 0;
            (Sludge False) -> 1;
    };

isBar :: Foo -> Bool;
isBar Bar = True;
isBar (Sludge _) = False;

main :: Query (Answer Foo);
main = do {
    f <- free;
    assert (isBar f);
    assert (1 == defoo f);
    query f;
}

}

