
module Seri.SMT.Tests.Query1 where

import Prelude
import Seri.SMT.SMT
import Seri.IO.IO
import Seri.IO.SMT

qmain :: Query (Answer Integer)
qmain = do
    x <- free
    assert (x < 6)
    assert (x > 4)
    query x

main :: IO ()
main = do
    putStrLn "yices1..."
    r1 <- runYices1 (Just "build/test/Query1.yices1.dbg") qmain
    putStrLn (show r1)

    putStrLn "yices2..."
    r2 <- runYices2 (Just "build/test/Query1.yices2.dbg") qmain
    putStrLn (show r2)

