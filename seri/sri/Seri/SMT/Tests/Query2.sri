
module Seri.SMT.Tests.Query2 where

import Prelude
import Seri.SMT.SMT

incr :: Integer -> Integer
incr a = a + 1

qmain :: Query (Answer Integer)
qmain = do
    x <- free
    assert (x < 6)
    assert (incr x > 5)
    query x

main :: IO ()
main = do
    putStrLn "yices1..."
    r1 <- runYices1 (Just "build/test/Query2.yices1.dbg") qmain
    putStrLn (show r1)

    putStrLn "yices2..."
    r2 <- runYices2 (Just "build/test/Query2.yices2.dbg") qmain
    putStrLn (show r2)

