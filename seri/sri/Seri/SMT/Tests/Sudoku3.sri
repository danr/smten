
module Seri.SMT.Tests.Sudoku3 where

import Prelude
import Seri.SMT.SMT
import Seri.Sudoku.Board
import Seri.Sudoku.Boards
import Seri.Sudoku.Solver
import Seri.Sudoku.BitCell

qmain :: Query [[Char]]
qmain = do
    board <- (readBoard diabolical :: Query (Board BitCell))
    result <- solve board
    case result of
       Satisfiable v -> return (print v)
       _ -> return ["no solution"]

main :: IO ()
main = do
    r <- runYices2 (Just "build/test/Sudoku3.yices2.dbg") qmain
    mapM_ putStrLn r

