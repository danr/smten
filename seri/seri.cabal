    Seri.Parser.Lexer
    Seri.Parser.Grammar
    Seri.Parser.PatOrExp
    Seri.Parser.Utils

    Seri.Loader

    Seri.Fresh.Pretty
    Seri.Fresh.Fast
    Seri.Fresh.Fresh

    Seri.SMT.IVP
    Seri.SMT.Solver
    Seri.SMT.Syntax
    Seri.SMT.Translate
    Seri.SMT.Query

    Seri.Fresh

    Seri.Ppr
    Seri.Lit
  other-modules: Paths_seri
  build-depends: base ==4.5.*, mtl ==2.1.*, pretty ==1.1.*, array ==0.4.*,
                 directory ==1.1.*, template-haskell ==2.7.*, hashable ==1.1.*,
                 containers ==0.4.*, bytestring ==0.9.*
  build-tools: happy
  ghc-prof-options: -fprof-auto-top
