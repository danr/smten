
module Seri.Lambda.TypeTest where {

fst :: forall a b . (a, b) -> a;
fst (x, _) = x;

snd :: forall a b . (a, b) -> b;
snd (_, y) = y;

(&&) :: Bool -> Bool -> Bool;
(&&) True True = True;
(&&) _ _ = False;

head :: forall a . [a] -> a;
head (x:_) = x;

tail :: forall a . [a] -> [a];
tail (_:xs) = xs

