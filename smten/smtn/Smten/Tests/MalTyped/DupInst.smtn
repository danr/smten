
module Smten.Tests.MalTyped.DupInst where

data Foo = Foo | Bar

-- Duplicate instances should not be allowed.
instance Eq Foo where
    (==) Foo Foo = True
    (==) Bar Bar = True
    (==) _ _ = False
    (/=) a b = not (a == b)

instance Eq Foo where
    (==) Foo Bar = True
    (==) Bar Foo = True
    (==) _ _ = False
    (/=) a b = not (a == b)

foo :: Bool
foo = Foo == Bar

