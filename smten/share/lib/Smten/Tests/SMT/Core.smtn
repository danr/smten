
module Smten.Tests.SMT.Core (tests) where

import Smten.Symbolic
import Smten.Tests.SMT.Test

tests :: IO ()
tests = do
   symtesteq "SMT.Core.Trivial" (Just ()) [Yices2] (return ())
   symtesteq "SMT.Core.Fail" Nothing [Yices2] (fail_symbolic :: Symbolic ())

   symtesteq "SMT.Core.FreeBool" (Just True) [Yices2] $ do
      p <- free_Bool
      assert p
      return p

   symtesteq "SMT.Core.JoinMaybe" (Just (Just ())) [Yices2] $ do
      p <- free_Bool
      assert p
      return $ if p then Just () else Nothing
    
   putStrLn "SMT.Core PASSED"
     

