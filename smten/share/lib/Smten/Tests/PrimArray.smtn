
module Smten.Tests.PrimArray where

import Smten.PrimArray
import Smten.Tests.Test

arr :: PrimArray String
arr = primArray ["foo", "bar", "sludge"]

testprimarray :: IO ()
testprimarray = do
    test "PrimArray 0" (primSelect arr 0 == "foo")
    test "PrimArray 1" (primSelect arr 1 == "bar")
    test "PrimArray 2" (primSelect arr 2 == "sludge")

